      "type" : "expression",
      "value" : [
        "if( $data.solicitacao ){",
        "\treturn {",
        "\t\t//map solicitacao variable to expected task contract input",
        "\t\tsolicitacaoInput: {",
        "\t\t\tnome: $data.solicitacao.nome !== undefined ? $data.solicitacao.nome : null,",
        "\t\t\tmatricula: $data.solicitacao.matricula !== undefined ? $data.solicitacao.matricula : null,",
        "\t\t\tcpf: $data.solicitacao.cpf !== undefined ? $data.solicitacao.cpf : null,",
        "\t\t\tcurso: $data.solicitacao.curso !== undefined ? $data.solicitacao.curso : null,",
        "\t\t\ttipo_declaracao: $data.solicitacao.tipo_declaracao !== undefined ? $data.solicitacao.tipo_declaracao : null,",
